{% extends "templates/prototype-template.njk" %}
{% set html_classes = "footer-at-end" %}
{% set body_classes = "index-layout" %}
{# set these for dev purposes #}
{% set layout_classes = "grid-visible baseline-grid" %}
{% set cols = 30 %}

{% block linked_css %}
  <link rel="stylesheet" href="/css/interior.css">
{% endblock %}

{% block main %}
  <section {% if first_section_class %}class="{{ first_section_class }}"{% endif %}>
    {% block column1 %}
      <h1>{% if page_title %}{{ page_title }}{% else %}{{ title }}{% endif %}</h1>
    {% endblock %}
  </section>
  <section {% if second_section_class %}class="{{ second_section_class }}"{% endif %}>
    {% block column2 %}
    {% endblock %}
  </section>
{% endblock %}
